<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="205.0" prefWidth="178.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.banksystem.Withdraw">
    <children>
        <Label text="Amount to withdraw">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
        <TextField fx:id="withdrawField" maxWidth="150.0" />
      <Label fx:id="limitLabel" />
      <Button fx:id="withdrawButton" mnemonicParsing="false" onAction="#confirmWithdraw" text="Withdraw" />
      <Button mnemonicParsing="false" onAction="#backFunctionality" text="Back" />
    </children>
</VBox>

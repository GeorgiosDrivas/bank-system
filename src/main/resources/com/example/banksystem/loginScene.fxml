<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.banksystem.loginSceneController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <TextField fx:id="emailText" maxWidth="150.0" promptText="Email" />
   <TextField fx:id="passwordText" maxWidth="150.0" promptText="Password" />

    <Label fx:id="welcomeText" />
    <Button onAction="#connectButton" text="Login" />
</VBox>
